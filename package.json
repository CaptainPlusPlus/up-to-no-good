{
  "name": "i-am-a-bad-friend",
  "displayName": "i am a bad friend",
  "description": "Ruin someone's day and repo with a single command!",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainPlusPlus/up-to-no-good.git"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "i-am-a-bad-friend.stupify",
        "title": "Convert Semicolons to Greek Question Marks"
      },
      {
        "command": "i-am-a-bad-friend.repero",
        "title": "Convert Greek Question Marks to Semicolons"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
